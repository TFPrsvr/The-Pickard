{
  "extends": [
    "next/core-web-vitals",
    "@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "react/prop-types": "error",
    "@typescript-eslint/no-unused-vars": "error",
    "prefer-const": "error",
    "react/no-unescaped-entities": "error"
  }
}